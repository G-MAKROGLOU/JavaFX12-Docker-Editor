<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane fx:id="rootPane" prefHeight="100.0" prefWidth="250.0" style="-fx-background-color: #282828; -fx-background-radius: 15;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="docker_management.DefaultDockerfile">
    <children>
        <VBox layoutX="22.0" layoutY="15.0" style="-fx-border-color: white; -fx-border-radius: 5;">
            <children>
                <ImageView fx:id="dockerLogo" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
        <TextFlow fx:id="dockerFileInformation" layoutX="93.0" layoutY="8.0" prefHeight="83.0" prefWidth="150.0" style="-fx-background-color: white; -fx-background-radius: 10;">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </TextFlow>
        <CheckBox fx:id="selectionCheckBox" disable="true" layoutX="14.0" layoutY="69.0" mnemonicParsing="false" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</AnchorPane>
